{
  "name": "sqlary",
  "description": "API server for salary information",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "GP",
    "email": "me@httgp.com",
    "url": "https://httgp.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node index.js",
    "dev": "LOG_LEVEL=debug ts-node index.ts",
    "format": "dprint fmt",
    "test": "tap --rcfile=./tests/.taprc",
    "test:only": "tap --rcfile=./tests/.taprc --only",
    "coverage": "npm run test -- --coverage --check-coverage --coverage-report=text --coverage-report=text-summary",
    "coverage:html": "npm run test -- --coverage --check-coverage --coverage-report=html"
  },
  "dependencies": {
    "@dnlup/fastify-traps": "2.0.0",
    "@fastify/auth": "4.1.0",
    "@fastify/bearer-auth": "8.0.1",
    "@fastify/swagger": "8.1.0",
    "@fastify/swagger-ui": "1.1.0",
    "ajv": "8.11.0",
    "better-sqlite3-helper": "3.1.6",
    "fastify": "4.9.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@swc/core": "1.3.11",
    "@types/node": "18.11.7",
    "@types/sinon": "10.0.13",
    "@types/tap": "15.0.7",
    "dprint": "0.32.2",
    "import-fresh": "3.3.0",
    "minifaker": "1.34.1",
    "nyc": "15.1.0",
    "sinon": "14.0.1",
    "source-map-support": "0.5.21",
    "tap": "16.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
